{
    "name": "schababerle-digital/shopware6-api-client",
    "description": "Ein PHP-Client fÃ¼r die Shopware 6 Admin API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Schababerle",
            "email": "patrick@schababerle.digital"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "phpunit/php-code-coverage": "*",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Shopware6Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shopware6Client\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}