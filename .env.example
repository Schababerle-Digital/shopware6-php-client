# Copy this file to .env and fill in your credentials
# Shopware API Configuration
SHOPWARE_URL=https://your-shopware-shop.com
SHOPWARE_CLIENT_ID=your-client-id-from-shopware-admin-api-access-keys
SHOPWARE_CLIENT_SECRET=your-client-secret-from-shopware-admin-api-access-keys

# Optional: Connection Settings
SHOPWARE_TIMEOUT=30
SHOPWARE_CONNECT_TIMEOUT=10

# Optional: Debug Settings
SHOPWARE_DEBUG=false
SHOPWARE_LOG_REQUESTS=false